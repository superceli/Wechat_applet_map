
map{
	width: 100%;
	height: calc(100vh - 226rpx);
}
.tobus{
	height: 266rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	padding: 20rpx 30rpx 30rpx 30rpx;
	background-color: #fff;
	margin: 0 0rpx;
	position: fixed;
	bottom: 0;
	left: 0;
	right: 0;
	-webkit-border-radius: 10rpx 10rpx 0 0;
	        border-radius: 10rpx 10rpx 0 0;
}
.tobus_box_title{
	font-size: 32rpx;
	font-weight: bold;
	color: #333333;
}
.txt_list{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	margin: 30rpx 0;
}
.txt_list text{
	font-size: 24rpx;
	color: #333;
	margin-right: 14rpx;
}
.tobus .tobus_box button{
	height: 84rpx;
	background: #1588ED;
	-webkit-border-radius: 10rpx;
	        border-radius: 10rpx;
	color: #fff;
	font-size: 32rpx;
}
.dw{
	-webkit-transform: translateY(-100%);
	        transform: translateY(-100%);
	top: -30rpx;
	position: absolute;
	right: 20rpx;
	width: 70rpx;
	height: 70rpx;
	bottom: 420rpx;
}

